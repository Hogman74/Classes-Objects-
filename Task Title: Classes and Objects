# python program to
# demonstrate defining
# a student class

class Student:
def __init__(self, name, age, tracks, score):

# user_input
self.name = str(name)
self.age = int(age)
self.tracks = list[tracks]
self.score = float(score)
print("My name is ", name)
print("I am ", age)
print("My track is ", tracks)
print("And My score is ", score)

# method

def introduce_self(self, name, age, tracks, score):
self.name = name
self.age = age
self.tracks = tracks
self.score = score
print("My name is ", name)
print("I am ", age)
print("My track is ", tracks)
print("And My score is ", score)

